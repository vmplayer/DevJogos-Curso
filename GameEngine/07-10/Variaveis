using System;

class Variaveis 
{
    static void Main (string[] args) 
    {
        //Aqui vai uma variavel de número INTEIRO, que é 25.
        int numero = 25;
        Console.WriteLine(numero);
        Console.WriteLine(numero + 14);
        Console.WriteLine(numero + 41);
        Console.WriteLine(numero + 621);
    }
}

//Comentário de uma linha só

